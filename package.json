{
  "name": "xs-ui",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm -C example dev",
    "docs": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --write .",
    "lint:css": "stylelint 'packages/theme-chalk/src/**/*.scss' --fix --custom-syntax postcss-scss",
    "lint:ls": "ls-lint",
    "clear": "rimraf dist",
    "clear:lib": "rimraf node_modules",
    "clear:cache": "rimraf node_modules/.cache/ rimraf node_modules/.vite",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "postinstall": "npx husky install",
    "reinstall": "rimraf pnpm-lock.yarm && rimraf node_modules && pnpm install",
    "commit": "git-cz",
    "build": "pnpm run -C build start"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "zhuiyue132",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@ls-lint/ls-lint": "^1.11.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "vite": "^3.2.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.2.36"
  },
  "dependencies": {
    "@xs-ui/components": "workspace:^0.0.1",
    "@xs-ui/config": "workspace:^0.0.1",
    "@xs-ui/eslint-config": "workspace:^0.0.1",
    "@xs-ui/hooks": "workspace:^0.0.1",
    "@xs-ui/theme-chalk": "workspace:^0.0.1",
    "@xs-ui/token": "workspace:^0.0.1",
    "@xs-ui/utils": "workspace:^0.0.1"
  }
}
