{
  "name": "xishui-ui",
  "private": true,
  "version": "1.0.7-beta.9",
  "scripts": {
    "dev": "pnpm -C example dev",
    "docs": "cross-env NODE_ENV=dev pnpm -C docs dev",
    "docs:build": "cross-env NODE_ENV=prod pnpm -C docs build",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --write .",
    "lint:css": "stylelint 'packages/theme-chalk/src/**/*.scss' --fix --custom-syntax postcss-scss",
    "lint:ls": "ls-lint",
    "clear": "rimraf dist",
    "clear:lib": "rimraf node_modules",
    "clear:cache": "rimraf node_modules/.cache/ rimraf node_modules/.vite",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "postinstall": "npx husky install",
    "reinstall": "rimraf pnpm-lock.yarm && rimraf node_modules && pnpm install",
    "commit": "git-cz",
    "build": "pnpm run -C build start",
    "release": "node release",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "zhuiyue132",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
